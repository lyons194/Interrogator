#Beginning of Interrogator
if 2 > 1:
        main_memory = []
        age = str(input("What is your age? "))
        while age.count("a") > 0:
                print("Error: Please enter an age that doesn't include a letter.")
                age = str(input("What is your age?"))
                main_memory.append(age)
                print(main_memory)
